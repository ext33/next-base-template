{
  "plugins": [
    "add-return-await",
    "apply-destructuring",
    "apply-optional-chaining",
    "convert-array-copy-to-slice",
    "convert-commonjs-to-esm",
    "convert-comparison-to-boolean",
    "convert-equal-to-strict-equal",
    "convert-esm-to-commonjs",
    "convert-for-each-to-for-of",
    "convert-for-in-to-for-of",
    "convert-for-to-for-of",
    "convert-generic-to-shorthand",
    "convert-index-of-to-includes",
    "convert-math-pow",
    "convert-object-assign-to-merge-spread",
    "convert-template-to-string",
    "convert-to-arrow-function",
    "convert-top-level-return",
    "merge-destructuring-properties",
    "merge-duplicate-imports",
    "merge-if-statements",
    "remove-console",
    "remove-constant-conditions",
    "remove-debugger",
    "remove-double-negations",
    "remove-duplicate-case",
    "remove-duplicate-interface-keys",
    "remove-duplicate-keys",
    "remove-empty-pattern",
    "remove-empty",
    "remove-iife",
    "remove-nested-blocks",
    "remove-process-exit",
    "remove-unreachable-code",
    "remove-unreferenced-variables",
    "remove-unused-expressions",
    "remove-unused-for-of-variables",
    "remove-unused-private-fields",
    "remove-unused-types",
    "remove-unused-variables",
    "remove-useless-arguments",
    "remove-useless-array-from",
    "remove-useless-async",
    "remove-useless-await",
    "remove-useless-escape",
    "remove-useless-for-of",
    "remove-useless-functions",
    "remove-useless-spread",
    "remove-useless-template-expressions",
    "remove-useless-typeof",
    "remove-useless-types",
    "remove-useless-types-from-constants",
    "remove-useless-variables",
    "simplify-ternary",
    "split-nested-destructuring",
    "split-variable-declarations",
    "react-hooks",
    "regexp",
    "promises",
    "declare-undefined-variables",
    "apply-nullish-coalescing",
    "apply-shorthand-properties",
    "apply-top-level-await",
    "convert-is-nan-to-number-is-nan"
  ],

  "rules": {
    "add-return-await": "on",
    "apply-destructuring/array": "on",
    "apply-destructuring/object": "on",
    "apply-nullish-coalescing": "off",
    "apply-optional-chaining": "on",
    "apply-top-level-await": "on",
    "convert-array-copy-to-slice": "off",
    "convert-commonjs-to-esm/commons": "on",
    "convert-commonjs-to-esm/exports": "on",
    "convert-commonjs-to-esm/require": "on",
    "convert-comparison-to-boolean": "on",
    "convert-equal-to-strict-equal": "off",
    "convert-esm-to-commonjs": "off",
    "convert-for-each-to-for-of": "off",
    "convert-for-in-to-for-of/negative": "on",
    "convert-for-in-to-for-of/positive": "on",
    "convert-for-to-for-of/length": "on",
    "convert-for-to-for-of/n": "on",
    "convert-generic-to-shorthand": "on",
    "convert-index-of-to-includes": "on",
    "convert-math-pow": "on",
    "convert-object-assign-to-merge-spread": "on",
    "convert-template-to-string": "off",
    "convert-to-arrow-function": "on",
    "convert-top-level-return": "off",
    "eslint/convert-ide-to-safe": "on",
    "eslint/move-putout-to-end-of-extends": "on",
    "extract-object-properties/equal-deep": "off",
    "extract-object-properties/not-equal-deep": "on",
    "merge-destructuring-properties": "on",
    "merge-duplicate-imports": "on",
    "merge-if-statements": "on",
    "promises/add-return-await": "on",
    "promises/convert-reject-to-throw": "on",
    "promises/remove-useless-resolve": "on",
    "react-hooks/convert-class-to-function": "off",
    "react-hooks/convert-component-to-use-state": "off",
    "react-hooks/convert-import-component-to-use-state": "off",
    "react-hooks/convert-state-to-hooks": "off",
    "react-hooks/remove-bind": "off",
    "react-hooks/remove-this": "off",
    "react-hooks/rename-method-under-score": "off",
    "regexp/apply-literal-notation": "on",
    "regexp/convert-replace-to-replace-all": "on",
    "regexp/convert-to-string": "on",
    "regexp/optimize": "on",
    "regexp/remove-useless-group": "on",
    "regexp/remove-useless-regexp": "on",
    "remove-console": "off",
    "remove-constant-conditions": "on",
    "remove-debugger": "off",
    "remove-double-negations": "off",
    "remove-duplicate-case": "on",
    "remove-duplicate-interface-keys": "on",
    "remove-duplicate-keys": "on",
    "remove-empty-pattern": "off",
    "remove-empty/argument": "on",
    "remove-empty/block": "on",
    "remove-empty/import": "off",
    "remove-empty/pattern": "on",
    "remove-iife": "on",
    "remove-nested-blocks": "off",
    "remove-process-exit": "off",
    "remove-unreachable-code": "off",
    "remove-unreferenced-variables": "on",
    "remove-unused-expressions": ["on", {"dismiss": ["strict mode"]}],
    "remove-unused-for-of-variables": "on",
    "remove-unused-private-fields": "on",
    "remove-unused-types": "on",
    "remove-unused-variables": "on",
    "remove-useless-arguments/arguments": "on",
    "remove-useless-arguments/destructuring": "on",
    "remove-useless-array-from": "on",
    "remove-useless-async": "on",
    "remove-useless-await": "on",
    "remove-useless-escape": "on",
    "remove-useless-for-of": "on",
    "remove-useless-functions": "on",
    "remove-useless-spread/array": "off",
    "remove-useless-spread/object": "off",
    "remove-useless-template-expressions": "on",
    "remove-useless-typeof": "on",
    "remove-useless-types": "on",
    "remove-useless-types-from-constants": "off",
    "remove-useless-variables/await": "on",
    "remove-useless-variables/for-of": ["on", {"maxProperties": 4}],
    "remove-useless-variables/remove": "on",
    "remove-useless-variables/rename": "on",
    "reuse-duplicate-init": "on",
    "simplify-ternary": "on",
    "split-nested-destructuring": "on",
    "split-variable-declarations": "off"
  },

  "ignore": [
    "**/node_modules",
    "**/fixture",
    "**/.nyc_output",
    "**/coverage",
    "**/dist",
    "**/dist-dev",
    "**/build",
    "**/package-lock.json",
    "src/locales",
    "src/styles"
  ]
}
